  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ngochoanit/trello-api/issues"
  },
  "homepage": "https://github.com/ngochoanit/trello-api#readme",
  "dependencies": {
    "@babel/runtime": "^7.14.8",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "joi": "^17.4.2",
    "lodash": "^4.17.21",
    "mongodb": "^3.6.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.14.9",
    "@babel/node": "^7.14.9",
    "@babel/plugin-transform-runtime": "^7.15.0",
